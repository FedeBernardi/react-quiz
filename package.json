    "url-loader": "^0.5.6",
    "eslint": "^2.12.0",
    "eslint-config-marlint": "^2.1.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "pre-push": "^0.1.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
